<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	<data-sources />
	<form-beans>
		
	    <form-bean name="insertProteinsForm" type="org.blastall.insert.action.InsertProteinsForm" />
	    
	</form-beans>

	<global-exceptions />
	
	<global-forwards>
	
		<forward name="insertProteins" path="/insertProteins.do" />
		
	</global-forwards>
	
	<action-mappings>
	
		<action path="/insertProteins" 
			type="org.blastall.insert.action.InsertProteinsAction"
			scope="request"
			validate="false"
			name="insertProteinsForm">
		</action>
		
	</action-mappings>
	
</struts-config>